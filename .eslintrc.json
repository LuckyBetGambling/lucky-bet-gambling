{
  "env": {
      "browser": true,
      "commonjs": true
  },
  "extends": "next/core-web-vitals",
  "rules": {
      "indent": [
          "error",
          "tab"
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "never"
      ]
  }
}